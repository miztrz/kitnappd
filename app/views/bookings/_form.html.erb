<%= form_with(model: [ @kitten, @kitten.bookings.build ], local: true) do |form| %>

  <div class="field">
    <%= form.label :start_time %>
    <%= form.date_field :start_time, type:'date' %>
  </div>

  <div class="field">
    <%= form.label :end_time %>
    <%= form.date_field :end_time, type:'date' %>
  </div>

  <div class="field">
    <%= form.label :total %>
    <%= form.text_field :total, readonly:true %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>